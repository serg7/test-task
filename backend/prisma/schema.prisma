generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  company   String
  address   String
  city      String
  createdAt DateTime @default(now())

  @@index([name])
  @@index([email])
  @@map("users")
}
